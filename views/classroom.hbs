<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>THIS IS CLASSROOM</title>

    <!-- socket.io jquery js stuff -->
    <script src="https://cdn.socket.io/socket.io-1.2.0.js"></script>
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="/javascripts/chat.js"></script>
</head>

<body>
    <div class='logo'><img src="images/logo.png" height="50px" /></div><br>
    <div class="container">
        <!-- 200px by 500px-->
        <!--Stoked Emoji-->
        <div class="actionBox">
            <span>
          <div class="emoji-reactions">  <!--popup description of emoji and white bqckground-->
          <ul><li>
          <span>
            <div class="reaction">
              <i class="react stoked"></i>
              <button class='rating' data-value='happy' type="button">Stoked</button>
            </div><div style="margin: 0 auto; width:175px;" class="right-align vertical-center" id="stoked">1 voting stoked</div>
            <div class="text">
              <div>Stoked</div>
            </div>
          </span>
            </li>
            </ul>
        </div>
        </span>
    </div>
    <br><br>
    <!--Happy Emoji-->
    <div class="actionBox">
        <span>
           <div class="emoji-reactions">  <!--popup description of emoji and white bqckground-->
           <ul><li>
           <span>
             <div class="reaction">
               <i class="react happy"></i>
               <button class='rating' data-value='ya' type="button">Happy</button>
             </div><div style="margin: 0 auto; width:175px;" class="right-align" id="happy">1 voting happy</div>
             <div class="text">
               <div>Happy</div>
             </div>
           </span>
        </li>
        </ul>
    </div>
    </span>
    </div>
    <br><br>
    <!--Ok Emoji-->
    <div class="actionBox">
        <span>
            <div class="emoji-reactions">  <!--popup description of emoji and white bqckground-->
            <ul><li>
            <span>
              <div class="reaction">
                <i class="react ok"></i>
                <button class='rating' data-value='meh' type="button">Ok</button>
              </div><div style="margin: 0 auto; width:175px;" class="right-align" id="ok">1 voting ok</div>
              <div class="text">
                <div>Ok</div>
              </div>
            </span>
        </li>
        </ul>
    </div>
    </span>
    </div>
    <br><br>
    <!--Lost Emoji-->
    <div class="actionBox">
        <span>
             <div class="emoji-reactions">  <!--popup description of emoji and white bqckground-->
             <ul><li>
             <span>
               <div class="reaction">
                 <i class="react sad"></i>
                 <button class='rating' data-value='confused' type="button">Sad</button>
               </div><div style="margin: 0 auto; width:175px;" class="right-align" id="sad">1 voting sad</div>
               <div class="text">
                 <div>Lost</div>
               </div>
             </span>
        </li>
        </ul>
    </div>
    </span>
    </div>
    <br><br>
    <!--Very Lost Emoji-->
    <div class="actionBox">
        <span>
              <div class="emoji-reactions">  <!--popup description of emoji and white bqckground-->
              <ul><li>
              <span>
                <div class="reaction">
                  <i class="react crying"></i>
                  <button class='rating' data-value='angry' type="button">Mad</button>
                </div><div style="margin: 0 auto; width:175px;" class="right-align" id="mad">1 voting mad</div>
                <div class="text">
                  <div>Very Lost</div>
                </div>
              </span>
        </li>
        </ul>
    </div>
    </span>
    </div>
    <br><br>
    </div>



</body>

</html>
